<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="javafxapplication1.CatalogueController">
    <stylesheets>
        <URL value="@styles.css" />
    </stylesheets>
   <children>
      <Label id="label" layoutX="241.0" layoutY="30.0" stylesheets="@styles.css" text="Catalogue">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <ImageView fitHeight="118.0" fitWidth="172.0" layoutX="24.0" layoutY="8.0" pickOnBounds="true" preserveRatio="true" style=": whitesmoke;">
         <image>
            <Image url="@../images/images.jpeg" />
         </image>
      </ImageView>
      <Label id="labe" layoutX="24.0" layoutY="143.0" stylesheets="@styles.css" text="View Movies:" />
      <Button id="btn" layoutX="24.0" layoutY="175.0" mnemonicParsing="false" onAction="#allmovies" prefHeight="25.0" prefWidth="155.0" stylesheets="@styles.css" text="All Movies" />
      <Button id="btn" layoutX="24.0" layoutY="217.0" mnemonicParsing="false" onAction="#availablemovies" prefHeight="25.0" prefWidth="155.0" stylesheets="@styles.css" text="Available Movies" />
      <Button id="btn" layoutX="22.0" layoutY="258.0" mnemonicParsing="false" onAction="#moviebygenres" prefHeight="25.0" prefWidth="155.0" stylesheets="@styles.css" text="Movies by Genre" />
      <Button id="btn" layoutX="22.0" layoutY="305.0" mnemonicParsing="false" onAction="#moviesbyyear" prefHeight="25.0" prefWidth="155.0" stylesheets="@styles.css" text="Movies by Year" />
      <Button id="btn" layoutX="199.0" layoutY="160.0" mnemonicParsing="false" onAction="#rentamovie" prefHeight="188.0" prefWidth="177.0" stylesheets="@styles.css" text="Rent a Movie" wrapText="true">
         <font>
            <Font size="29.0" />
         </font>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </Button>
      <Button id="btn" layoutX="396.0" layoutY="160.0" mnemonicParsing="false" onAction="#returnamovie" prefHeight="188.0" prefWidth="177.0" stylesheets="@styles.css" text="Return a Movie" wrapText="true">
         <font>
            <Font size="29.0" />
         </font>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </Button>
      <Button id="btnb" layoutX="287.0" layoutY="361.0" mnemonicParsing="false" onAction="#exit" stylesheets="@styles.css" text="Exit" />
   </children>
</AnchorPane>
